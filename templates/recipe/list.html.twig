{% extends "base.html.twig" %}

{% block body %}


{% for recipe in recipies %}
<div class="recipe">
    <a href="{{ path('recipe_view', {'id': recipe.id}) }}">
        {{ recipe.name }}
    </a>
    <a href="{{ path('recipe_update',{'id': recipe.id}) }}" class="button">
        <span class="backto">Editer</span>
    </a>
</div>
{% endfor %}


{% endblock %}